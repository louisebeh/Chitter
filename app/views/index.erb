<ul>
  <% @peeps.each do |peep| %>
    <li>
    <%= peep.message %>
    <a href="<%= peep.username %>"></a>
    <a href="<%= peep.name %>"></a>
    </li>
  <% end %>
</ul>

<br>

<% if current_maker %>
  <peep id="new-peep">
    <form action="/peeps" method="post">
      What's Peepin'?: <input type="text" name="message">
      <input type="submit" value="Peep!">
    </form>
  </peep>
<% end %>